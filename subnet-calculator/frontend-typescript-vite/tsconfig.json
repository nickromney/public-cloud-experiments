{
  "extends": "../shared-frontend/tsconfig.json",
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.tsbuildinfo",
    "allowImportingTsExtensions": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "paths": {
      "@subnet-calculator/shared-frontend": ["../shared-frontend/src/index.ts"],
      "@subnet-calculator/shared-frontend/*": ["../shared-frontend/src/*"]
    }
  },
  "include": ["src/**/*", "src/**/*.ts"],
  "references": [{ "path": "../shared-frontend" }]
}
